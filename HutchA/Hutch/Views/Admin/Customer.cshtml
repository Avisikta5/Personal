@model IEnumerable<Hutch.Models.User>

@{
    ViewBag.Title = "Customer";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<link href="~/Content/Styles/Admin/Customer.css" rel="stylesheet" />

<main class="admin-all-users-container">

    <div class="all-users">

        <div class="user-card-container">
            @foreach (var user in Model)
            {
                <div class="user-card">

                    <div>
                        <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="#000000" viewBox="0 0 256 256">
                            <path d="M229.19,213c-15.81-27.32-40.63-46.49-69.47-54.62a70,70,0,1,0-63.44,0C67.44,166.5,42.62,185.67,26.81,213a6,6,0,1,0,10.38,6C56.4,185.81,90.34,166,128,166s71.6,19.81,90.81,53a6,6,0,1,0,10.38-6ZM70,96a58,58,0,1,1,58,58A58.07,58.07,0,0,1,70,96Z"></path>
                        </svg>
                    </div>

                    <div class="user-details">
                        <h5>@user.name</h5>
                        <h6>@user.email</h6>
                    </div>

                </div>
            }
        
    </div>
</main>
